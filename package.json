{
  "name": "codexer",
  "version": "0.1.0",
  "description": "Wrapper CLI for Codex session management",
  "type": "module",
  "bin": {
    "codexer": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format esm --platform node --dts false",
    "dev": "tsx src/cli.ts",
    "lint": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.2.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
